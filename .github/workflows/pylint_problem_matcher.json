{
    "problemMatcher": [
        {
            "owner": "pylint_warning",
            "severity" : "warning",
            "pattern": [
                {
                    "regexp": "^([^:]+):(\\d+):(\\d+):\\s([^E][^:]+):\\s(.+)$",
                    "file" : 1,
                    "line" : 2,
                    "column" : 3,
                    "code" : 4,
                    "message": 5
                }
            ]
        },
        {
            "owner": "pylint_error",
            "severity" : "error",
            "pattern": [
                {
                    "regexp": "^([^:]+):(\\d+):(\\d+):\\s(E[^:]+):\\s(.+)$",
                    "file" : 1,
                    "line" : 2,
                    "column" : 3,
                    "code" : 4,
                    "message": 5
                }
            ]
        }
    ]
}
